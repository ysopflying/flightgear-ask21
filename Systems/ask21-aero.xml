						14	1
					</tableData>
				</table>
		</function>			
		<function name="aero/function/vnl">
			<description>v normal left</description>
			<sum>
			<product>
				<property>ic/v-fps</property>
				<value>0.9976</value>
			</product>
			<product>
				<property>ic/w-fps</property>
				<value>-0.0698</value>
			</product>
			</sum>
		</function>			

		<function name="aero/function/vnr">
			<description>v normal right</description>
			<sum>
			<product>
				<property>ic/v-fps</property>
				<value>0.9976</value>
			</product>
			<product>
				<property>ic/w-fps</property>
				<value>0.0698</value>
			</product>
			</sum>
		</function>			

		<function name="aero/function/local_alpha_sideslip_left">
			<description>change local alpha due to sideslip left</description>
			<atan2>
				<property>aero/function/vnl</property>
				<property>ic/u-fps</property>
			</atan2>
		</function>			
		<function name="aero/function/local_alpha_sideslip_right">
			<description>change local alpha due to sideslip right</description>
			<atan2>
				<property>aero/function/vnr</property>
				<property>ic/u-fps</property>
			</atan2>
		</function>			

		<function name="aero/function/local_alphachange_roll1l">
			<description>change local alpha due to roll</description>
			<quotient>
				<product>
					<property>velocities/p-aero-rad_sec</property>
					<value>-1</value>
					<value>10</value> <!-- y1[ft]-->
				</product>
			<property>velocities/u-aero-fps</property>
		</quotient>

		</function>			
		<function name="aero/function/local_alphachange_roll1r">
			<description>change local alpha due to roll</description>
			<product>
				<property>aero/function/local_alphachange_roll1l</property>
				<value>-1</value> <!-- y1[ft]-->
			</product>
		</function>			


		<function name="aero/function/local_u1l">
			<description>local u due to r</description>
			<sum>
			<product>
				<property>velocities/r-aero-rad_sec</property>
				<value>1</value> <!-- y1[ft]-->
			</product>
			<property>ic/u-fps</property>
			</sum>
		</function>			

		<function name="aero/function/local_beta_tail">
			<description>local beta for vertical stabilizer</description>
				<value>1</value>
		</function>

