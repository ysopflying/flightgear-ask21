<?xml version="1.0"?>

<PropertyList>
	
	<path>panelR.ac</path>
	
	<!--COCKPIT-->
	<!--FLARM-->
	<model>
		<name>flarm</name>
		<path>Aircraft/Instruments-3d/glider/flarm/traditional/flarm_traditional.xml</path>
		<offsets>
			<x-m>-2.2</x-m>
			<y-m>0.0</y-m>
			<z-m>0.89</z-m>
			<pitch-deg>-10</pitch-deg>
		</offsets>
	</model>
	<!--REAR INSTRUMENTS-->
	<!--Altimeter-->
	<model>
		<name>alt</name>
		<path>Instruments/alti-4110/winter-4110.xml</path>
		<offsets>
			<x-m>-2.18746</x-m>
			<y-m>-0.06</y-m>
			<z-m>0.73</z-m>
			<pitch-deg>-5.5</pitch-deg>
		</offsets>
	</model>
	<model>
		<name>alt-1</name>
		<path>Instruments/alt/Altimeter.xml</path>
		<offsets>
			<x-m>-2.18746</x-m>
			<y-m>-0.06</y-m>
			<z-m>0.73</z-m>
			<pitch-deg>-5.5</pitch-deg>
		</offsets>
	</model>
	<animation>
		<type>select</type>
		<object-name>alt</object-name>
		<condition>
			<equals>
				<property>/sim/model/livery/instruments/alt</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>alt-1</object-name>
		<condition>
			<equals>
				<property>/sim/model/livery/instruments/alt</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	<!--ASI-->
	<model>
		<name>asi2</name>
		<path>Instruments/winter-asi-ask21-kph/80mm_asi_6fms4.xml</path>
		<offsets>
			<x-m>-2.193</x-m>
			<y-m>-0.11</y-m>
			<z-m>0.819</z-m>
			<pitch-deg>-5.5</pitch-deg>
		</offsets>
	</model>
	<model>
		<name>asi2-1</name>
		<path>Instruments/winter-asi-6221/winter-asi-6221.xml</path>
		<offsets>
			<x-m>-2.193</x-m>
			<y-m>-0.11</y-m>
			<z-m>0.819</z-m>
			<pitch-deg>-5.5</pitch-deg>
		</offsets>
	</model>
