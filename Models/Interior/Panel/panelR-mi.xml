			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>asi2-1</object-name>
		<condition>
			<equals>
				<property>/sim/model/livery/instruments/asi</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>asi2-2</object-name>
		<condition>
			<equals>
				<property>/sim/model/livery/instruments/asi</property>
				<value>2</value>
			</equals>
		</condition>
	</animation>
	<!--Variometer-->
	<model>
		<name>vario</name>
		<path>Instruments/vario-5stv5-kmph/winter-vario-5stv5.xml</path>
		<offsets>
			<x-m>-2.19714</x-m>
			<y-m>0.11</y-m>
			<z-m>0.819</z-m>
			<pitch-deg>-5.5</pitch-deg>
		</offsets>
	</model>
